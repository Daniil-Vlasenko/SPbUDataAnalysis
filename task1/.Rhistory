library(readxl)
library(dplyr)
df <- read_xls("PRESTIGE (1).xls")
head(df)
library(ggplot2)
library(GGally)
df %>% select(PRESTIGE, SUICIDE, INCOME, SCHOOL) %>%
ggpairs(diag=list(continuous = "barDiag"))
df %>% select(PRESTIGE, SUICIDE, INCOME, SCHOOL) %>%
mutate(INCOME = log(INCOME)) %>%
ggpairs(diag=list(continuous = "barDiag"))
model = lm(SUICIDE~PRESTIGE+INCOME+SCHOOL, data=df)
summary(model)
library(lm.beta)
model.st <- lm.beta(model)
summary(model.st)
vcov(model)
cov2cor(vcov(model))
